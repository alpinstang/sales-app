@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1><img class="img-responsive" src="data:image/png;base64,"  title="Fleet SalesBase" /></h1>
    <p class="lead">A Streamlined Sales Planning Tool from Fleet Safety Equipment, Inc.</p>
    @if (User.Identity.IsAuthenticated && ViewContext.HttpContext.User.IsInRole("Admin") || User.Identity.IsAuthenticated && ViewContext.HttpContext.User.IsInRole("User"))
    {
        <p>If you were affected by the user database updates, please change your password to something more secure than the defaulted password using the 'manage account' link at the top.</p>
        <p><a href="/SalesApp/ToDo/Create" class="btn btn-primary btn-large">Create New Appointment</a></p>
        <p><a href="/SalesApp/ToDo/Index" class="btn btn-primary btn-large">View Appointments</a></p>
        <p><a href="/SalesApp/AfterVisit/Create" class="btn btn-primary btn-large">Create New After Visit Report</a></p>
        <p><a href="/SalesApp/AfterVisit/Index" class="btn btn-primary btn-large">View After Visit Reports</a></p>
    }
    else if (User.Identity.IsAuthenticated)
    {
        <p>Please email <a href="mailto:john@fleetsafety.com">john@fleetsafety.com</a> with your user name to complete your account setup.</p>
    }
    else
    {
        <p><a href="/SalesApp/Account/Login" class="btn btn-primary btn-large">Login</a></p>
        <p>New? Register by clicking the button below.</p>
        <p><a href="/SalesApp/Account/Register" class="btn btn-primary btn-large">Register</a></p>
    }

</div>
<h3>Help &amp; Features</h3>
<ol>
    <li>
            <p>This application is designed to streamline sales planning and help coordinate sales representatives efforts across territories.</p>
            <h4>Quick Start</h4>
            <ul>@if (User.Identity.IsAuthenticated && ViewContext.HttpContext.User.IsInRole("Admin") || User.Identity.IsAuthenticated && ViewContext.HttpContext.User.IsInRole("User"))
                {
                    <li><b>Appointment</b> - Schedule a future date to meet a customer.</li>
                    <li><b>After Visit</b> - Report sales progress after an appointment.</li>
                    <li>Go forth and sell!</li>
                }
                else if (User.Identity.IsAuthenticated)
                {
                    <li>Register, then send an email to <a href="mailto:john@fleetsafety.com">john@fleetsafety.com</a> with your user name you created.</li>
                }
                
                
            </ul>
        @if (User.Identity.IsAuthenticated && ViewContext.HttpContext.User.IsInRole("Admin"))
            {
